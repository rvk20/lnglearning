{% extends 'baseuser.html.twig' %}

{% block title %}Czytanie ze zrozumieniem{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper" style="display: block;
  margin-left: auto;
  margin-right: auto;
  width: 60%;
  border: 0;
  border-radius: 1rem;
  box-shadow: 0 0.5rem 1rem 0 rgba(0, 0, 0, 0.1);
  padding-top: 30px;
  padding-right: 30px;
  padding-bottom: 30px;
  padding-left: 30px;
  ">
   
     {% for quiz in quizs %}
     
   <div class="example-wrapper" style="display: block;
  margin-left: auto;
  margin-right: auto;
  width: 50%;">
   <a name="submit" href="readingcomp/{{quiz.id}}" type="submit" id="{{quiz.id}}" class="btn btn-primary btn-lg">{{ quiz.name }} ({{quiz.level}}) </a>
</div>
    {% endfor %}  

  
</div>
{% endblock %}
